<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
    <field name="prenom">
        <field-validator type="requiredstring">
            <message key="error.prenom.required"/>
        </field-validator>
    </field>
    <field name="nom">
        <field-validator type="requiredstring">
            <message key="error.nom.required"/>
        </field-validator>
    </field>
    <field name="telephone">
        <field-validator type="requiredstring">
            <message key="error.telephone.required"/>
        </field-validator>
    </field>
</validators>
